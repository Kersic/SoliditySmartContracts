  "source": "pragma solidity ^0.8.1;\n\ncontract Ownable {\n    address payable _owner;\n\n    constructor() public {\n        _owner = payable(msg.sender);\n    }\n\n    modifier onlyOwner() {\n        require(msg.sender == _owner, \"You are not allowed\");\n        _;\n    }\n\n    function isOwner() public view returns (bool) {\n        return msg.sender == _owner;\n    }\n}",
